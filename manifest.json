{
    "manifest_version": 3,
    "name": "Notes Finder",
    "version": "1.0",
    "description": "Generate subject-wise questions using AI based on keywords, standard, and question type.",
    "permissions": [
        "storage"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "128": "icon.png"
        }
    },
    "icons": {
        "128": "icon.png"
    },
    "host_permissions": [
        "https://api.cohere.ai/*"
    ],
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    }
}
  